# Charging
genfscon sysfs /devices/platform/mtk_ctd/power_supply                   u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/odm/odm:tran_auto_test/power_supply    u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/odm/odm:usb_switch/power_supply        u:object_r:sysfs_batteryinfo:s0

# Gesture
genfscon proc /game_state          u:object_r:proc_main_gesture_flag:s0
genfscon proc /gesture_function    u:object_r:proc_gesture_function:s0

# Torch
genfscon sysfs /devices/virtual/sub_torch/sub_torch/sub_torch_level       u:object_r:sysfs_sub_torch_file:s0
genfscon sysfs /devices/virtual/tele_torch/tele_torch/tele_torch_level    u:object_r:sysfs_tele_torch_file:s0
genfscon sysfs /devices/virtual/torch/torch/torch_level                   u:object_r:sysfs_torch_file:s0
genfscon sysfs /devices/virtual/wide_torch/wide_torch/wide_torch_level    u:object_r:sysfs_wide_torch_file:s0

